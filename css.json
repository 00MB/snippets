	"scss skeleton": {
		"prefix": "skeleton",
		"body": [
			"/*Lato font*/",
			"@import url('https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,300;0,400;1,400&display=swap');\n",
			"/*DM sans font*/",
			"@import url('https://fonts.googleapis.com/css2?family=DM+Sans:ital,wght@0,400;0,500;0,700;1,400;1,500;1,700&display=swap');\n",
			"body {background-color:'black'}"
		],
		"description": "skeleton for css"
	}
	
